.water-mark-mask {
  position: fixed;
  top: 0;
  left: 45rpx;
  right: 0;
  bottom: 0;
  z-index: 9999;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  pointer-events: none; 
  flex:1;
  font-size: 30rpx;
  color:#D3D3D3;
}
 
.row{
  display: flex;
  flex-direction: column;
  align-items: center; 
  /* justify-content: space-between;  */
}
.col{
  transform: rotate(-45deg);
  height:900rpx;
}
