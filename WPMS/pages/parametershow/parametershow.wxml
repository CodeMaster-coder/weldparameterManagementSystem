</view>
<scroll-view enable-flex="true" class='scroll-view'  scroll-x>
<view wx:for="{{spotparam}}" wx:key = 'unique' class="content">
  
<view class="subcontent">{{gun_name}}</view>
<view class="subcontent">{{carType_name}}</view>
<view class="subcontent1 font-break">{{item.weldSpotNumber}}</view>
<view class="subcontent">{{item.weldSpotProgram}}</view>
<view class="subcontent">{{item.sheet1}}</view>
<view class="subcontent">{{item.sheet2}}</view>
<view class="subcontent">{{item.sheet3}}</view>
<view class="subcontent">{{item.sheet4}}</view>
<view class="subcontent">{{item.preloadingTime}}</view>
<view class="subcontent">{{item.preheatingTime}}</view>
<view class="subcontent">{{item.preheatingCurrent}}</view>
<view class="subcontent">{{item.coolingTime}}</view>
<view class="subcontent">{{item.risslopeTime}}</view>
<view class="subcontent">{{item.startCurrent}}</view>
<view class="subcontent">{{item.endCurrent}}</view>
<view class="subcontent">{{item.mainWeldTime}}</view>
<view class="subcontent">{{item.mainWeldCurrent}}</view>
<view class="subcontent">{{item.downslopeTime}}</view>
<view class="subcontent">{{item.startCurrent1}}</view>
<view class="subcontent">{{item.endCurrent1}}</view>
<view class="subcontent">{{item.impulse}}</view>
<view class="subcontent">{{item.intervalTime}}</view>
<view class="subcontent">{{item.holdTime}}</view>
<view class="subcontent">{{item.weldPressure}}</view>
<view class="subcontent">{{item.dressingPoints}}</view>
<view class="subcontent">{{item.dressingNumbers}}</view>

</view>
</scroll-view>
</view>
